/ Modal
#hintModal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "exampleModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog{:role => "document"}
    .modal-content
      .modal-header
        %h5.modal-title= t('user.app_user_info')
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", :type => "button"}
          %span{"aria-hidden" => "true"} Ã—

      .modal-body
        = t('user.go_to_app')
        %ol
          %li= t('user.user_uuid')
          %li= t('user.user_fullname')

        .w-75.m-auto.mt-3
          = image_tag "app_user_profile.jpg", style: 'width: 100%'

      .modal-footer
        %button.btn.btn-secondary{"data-dismiss": "modal", type: "button"}= t('shared.close')
